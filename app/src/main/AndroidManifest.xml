        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ContactStudent"
            android:label=""
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Design.NoActionBar"
            android:parentActivityName=".Librarian_Book_Info"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".Librarian_Book_Info"
            android:label=""
            android:parentActivityName=".LibrarianHomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Design.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".Student_View_Issued_Return"
            android:label=""
            android:parentActivityName=".StudentHomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Design.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
