            android:name=".Student_Book_Info"
            android:label=""
            android:parentActivityName=".StudentHomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Design.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity android:name=".SucessfullyReturn" />
        <activity
            android:name=".Librarian_View_Issued_Return"
            android:label=""
            android:parentActivityName=".LibrarianHomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Design.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".View_Issued_Books"
            android:label=""
